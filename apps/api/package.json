    "@types/supertest": "^2.0.12",
    "@types/turndown": "^5.0.1",
    "eslint": "^7.32.0",
    "eslint-config-custom-server": "*",
    "jest": "^26.6.3",
    "prisma": "^4.13.0",
    "supertest": "^6.2.4",
    "tsconfig": "*",
    "tsup": "^6.2.3",
    "typescript": "^4.8.3"
