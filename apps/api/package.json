    "dotenv": "^16.0.3",
    "express": "^4.17.1",
    "express-validator": "^7.0.1",
    "gpt3-tokenizer": "^1.1.5",
    "jsonwebtoken": "^9.0.0",
    "morgan": "^1.10.0",
    "openai-edge": "^0.5.1",
    "puppeteer": "^20.1.1",
    "turndown": "^7.1.2",
